{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    // Remove .fvm files from search
    "search.exclude": {
      "**/.fvm": true
    },
    // Remove from file watching
    "files.watcherExclude": {
      "**/.fvm": true
    },
    // Fix actions on save
    "editor.codeActionsOnSave": {
      "source.fixAll": true
   },
   // Format on save
   "editor.formatOnSave": true
  }